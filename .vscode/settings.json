{
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "git.ignoreLimitWarning": true,
  "java.configuration.updateBuildConfiguration": "interactive",
  "liveServer.settings.multiRootWorkspaceName": "CalcCost",
  "java.compile.nullAnalysis.mode": "automatic"
}
